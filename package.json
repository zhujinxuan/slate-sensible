    "slate-delete-one-line": "*",
    "slate-on-select": "*",
    "slate-plain-serializer": "*",
    "slate-poor-mentions": "*",
    "slate-react": "*",
    "slate-schema-violations": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
